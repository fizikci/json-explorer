{
  "name": "json-explorer",
  "publisher": "BulentKeskin",
  "displayName": "JSON Explorer",
  "icon": "media/icon.png",
  "description": "Effortlessly explore, visualize, and interact with JSON data directly within VS Code",
  "version": "0.0.2",
  "repository": {
    "url": "https://github.com/fizikci/json-explorer"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "jsonExplorer.openWith",
          "group": "navigation",
          "when": "resourceLangId == json"
        }
      ]
    },
    "commands": [
      {
        "command": "jsonExplorer.openWith",
        "title": "Open with JSON Explorer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}